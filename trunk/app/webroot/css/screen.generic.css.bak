}

/*****************************************************************************/

p.error {
    background: #ffe5e5;
    color: #b20000;
    margin: 1em 0;
    border: 1px solid #235f7d;
    padding: 0.5em;
}

p.notice {
    background: #fffee5;
    margin: 1em 0;
    border: 1px solid #235f7d;
    padding: 0.5em;
}

p.success {
    background: #aed8ed;
    margin: 1em 0;
    border: 1px solid #235f7d;
    padding: 0.5em;
}

.notice a {
    color: blue;
}

/*****************************************************************************/

table#searchResults a {
    display: block;
}

/* FIXME ?? */
ul.tabs {
    display: block;
    padding: 0;
    margin: 0;
    list-style: none;
    margin: 1em 0 0 0;
}

ul.tabs li {
    display: inline;
    padding: 0 0.25em;
    margin: 0;
    border: 1px solid #235f7d;
    background: #f0f0f0;
}

ul.tabs li.selected {
    border-bottom: 1px solid white;
    background: white;
}

/*****************************************************************************/

table.tooltips tbody tr {
    position: relative;
}

table.tooltips thead tr th.tooltip_table, table.tooltips tbody tr td.tooltip_table {
    display: none;
}

table.tooltips tbody tr.hover td {
    cursor: help;
}

table.tooltips tbody tr.hover td.tooltip_table {
    position: absolute;
    display: block;
    border: 1px solid #235f7d;
    background: white;
    left: 0;
    top: auto;
    z-index: 5;
    padding: 0;
}

table.tooltips td table {
    padding: 0;
    margin: 0;
}

/*****************************************************************************/

div.clearer {
    clear: both;
}

div.clearer hr {
    display: none;
}

/*****************************************************************************/

.with_treemenu {
    margin-left: 18em;
}

.treemenu {
    width: 17em;
    border: 1px solid #235f7d;
    float: left;
}

.treemenu ul {
    display: block;
    margin: 0;
    padding: 0;
    list-style: none;
    background: #e6eef2;
}

.treemenu li {
    display: block;
    position: relative;
    margin: 0;
    padding: 0;
    padding-left: 1em;
}

.treemenu a, .treemenu span {
    display: block;
    padding: 0.125em 0;
}

.treemenu a.toggler {
    display: block;
    width: 1em;
    height: 1.2em;
    line-height: 1.2em;
    position: absolute;
    z-index: 5;
    top: 0;
    left: 0;
    cursor: pointer;
}

.treemenu li li {
    display: block;
}

.treemenu h2 {
    font-size: 1em;
    background: #235f7d;
    padding: 0.25em;
    border-bottom: 1px solid #235f7d;
}

.treemenu h2 a {
    color: white;
}

/*.treemenu li, .treemenu li ul { background: #f9ffe6; }
.treemenu li li, .treemenu li li ul { background: white; }*/

/*****************************************************************************/

ul.actionMenu {
    margin: 0.75em 0;
    padding: 0;
    list-style: none;
}

ul.actionMenu li {
    margin: 0 0.5em 0 0;
    padding: 0;
    display: inline;
}

ul.actionMenu a {
    padding: 0.33em 0.5em;
    border: 1px solid #235f7d;
    display: inline-block;
    text-decoration: none;
    color: black;
}

table a {
    display: inline-block;
}

/*****************************************************************************/

#pageMenu * {
    line-height: 1em;
}

#pageMenu ul {
    list-style: none;
    padding: 0;
    margin: 0;
    position: relative;
    border-bottom: 1px solid silver;
    height: 2em;
}

#pageMenu li {
    padding: 0;
    margin: 0;
    float: left;
    padding: 0.5em 0;
}

#pageMenu a {
    border-right: 1px solid silver;
    padding: 0.5em;
}

#pageMenu ul ul {
    display: none;
}

#pageMenu li:hover ul {
    display: block;
    position: absolute;
/*     left: 0; */
    top: 2em;
    margin-top: 2px;
}

#pageMenu li:hover a {
    background: #9999ff;
}

#pageMenu li:hover ul a {
    background: white;
}

#pageMenu li a {
    border: 0;
}


