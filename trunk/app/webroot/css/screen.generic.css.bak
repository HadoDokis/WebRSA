#flashMessage {
    margin: 1em 0;
    border: 1px solid #235f7d;
    padding: 0.5em;
}

#flashMessage.message {
    background: #fffee5;
}

#flashMessage.error {
    background: #ffe5e5;
    color: #cc0000;
}

#flashMessage.success {
